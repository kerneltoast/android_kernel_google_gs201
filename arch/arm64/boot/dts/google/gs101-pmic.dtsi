					schematic-name = "VSYS_PWR_RFFE";
					subsys-name = "Cellular";
				};
			};
		};

		s2mpg10_gpio: s2mpg10_gpio {
			gpio-controller;
			#gpio-cells = <2>;
			ngpios = <6>;
			gpio-ranges = <&s2mpg10_pinctrl 0 0 6>;
		};

		s2mpg10_pinctrl: s2mpg10_pinctrl {
		};
	};
};

