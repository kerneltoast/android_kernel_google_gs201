#include "gs101-pinctrl.dtsi"

/ {
	sbb_mux: sbb-mux {
		compatible = "google,sbb-mux";
		gpios = <&gph2 2 0>, <&gph2 3 0>, <&gpp0 4 0>;
		gpio_names = "C1_T1__XHSI2_GPIO0", "C1_T2__XHSI2_GPIO1",
		    "C1_T3__XAPC_DISP_TE_SEC";
		default_signals = "gp_region_0", "gp_region_1",
		    "gp_region_2";
	};

};
